{
  "auth": {
    "Meep": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "customAuth": false
    }
  },
  "function": {
    "pushBroadcast": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "api": {
    "broadcastsApi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "pushBroadcast",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "MeepAPI": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "AMAZON_COGNITO_USER_POOLS",
            "userPoolConfig": {
              "userPoolId": "authMeep"
            }
          },
          "additionalAuthenticationProviders": [
            {
              "authenticationType": "API_KEY",
              "apiKeyConfig": {
                "apiKeyExpirationDays": 365,
                "description": "public Api"
              }
            }
          ]
        }
      }
    }
  },
  "storage": {},
  "analytics": {
    "meep": {
      "service": "Pinpoint",
      "providerPlugin": "awscloudformation"
    }
  }
}