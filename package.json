{
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest --verbose",
    "test:watch": "npm run test --watch",
    "test:ci": "npm run test --coverage",
    "test:badges": "npm run test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__"
  },
  "jest": {
    "preset": "jest-expo",
    "globals": {
      "ts-jest": {
        "compiler": "ttypescript",
        "tsconfig": {
          "jsx": "react"
        }
      }
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.jsx$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|rn-sliding-up-panel|rn-swipe-gestures)"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "setupFiles": [
      "<rootDir>/src/testing/config.tsx"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.0",
    "@iconify-icons/eva": "^1.1.0",
    "@iconify/react": "^1.1.4",
    "@react-native-async-storage/async-storage": "^1.13.0",
    "@react-native-community/checkbox": "^0.5.7",
    "@react-native-community/datetimepicker": "3.2.0",
    "@react-native-community/netinfo": "6.0.0",
    "@react-native-community/slider": "3.0.3",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "@sharcoux/slider": "^5.6.1",
    "@types/google-libphonenumber": "^7.4.20",
    "aws-amplify": "^3.3.26",
    "expo": "^41.0.0",
    "expo-checkbox": "~1.0.3",
    "expo-clipboard": "~1.0.2",
    "expo-contacts": "~9.1.2",
    "expo-firebase-analytics": "~4.0.2",
    "expo-font": "~9.1.0",
    "expo-image-picker": "~10.1.4",
    "expo-location": "~12.0.4",
    "expo-notifications": "~0.11.6",
    "expo-secure-store": "~10.1.0",
    "expo-splash-screen": "~0.10.2",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.5.4",
    "formik": "^2.2.6",
    "google-libphonenumber": "^3.2.19",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "qs": "^6.10.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "0.63.4",
    "react-native-check-box": "^2.1.7",
    "react-native-contacts": "^6.0.5",
    "react-native-date-picker": "^3.3.2",
    "react-native-elements": "^3.4.1",
    "react-native-expo-viewport-units": "^0.0.8",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-google-places-autocomplete": "^2.2.0",
    "react-native-maps": "0.27.1",
    "react-native-reanimated": "~2.1.0",
    "react-native-screens": "~3.0.0",
    "react-native-svg": "12.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-unimodules": "~0.13.3",
    "react-native-uuid": "^2.0.0",
    "react-native-web": "~0.13.12",
    "rn-sliding-up-panel": "^2.4.5",
    "rn-swipe-gestures": "^1.0.3",
    "unirest": "^0.6.0",
