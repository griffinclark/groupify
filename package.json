  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@jest/reporters": "^27.4.6",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^27.0.2",
    "@types/qs": "^6.9.6",
    "@types/react": "~16.9.35",
