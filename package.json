    "jest-expo": "^42.1.0",
    "jest-ts-auto-mock": "^2.0.0",
    "prettier": "^2.3.0",
    "react-test-renderer": "^16.14.0",
    "regenerator-runtime": "^0.13.9",
    "ts-auto-mock": "^3.5.0",
    "ts-jest": "^26.5.4",
    "ttypescript": "^1.5.12",
    "typescript": "~4.0.0"
  },
  "private": true,
  "name": "meep",
  "version": "1.0.0"
}
