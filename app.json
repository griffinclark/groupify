{
  "expo": {
    "name": "meep",
    "slug": "meep",
    "sdkVersion": "40.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.munchkinlabs.meep",
      "infoPlist": {
        "NSContactsUsageDescription": "Accessing your contacts will allow you to invite your friends to events you create"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.munchkinlabs.meep"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
